!v702!C:\SciELO\Serial\rsp\v38n3\markup\20666.htm
!v704!<p><font face="Verdana, Arial, Helvetica-Normal, sans-serif" size="3"><b>M&Eacute;TODOS</b></font></p>     ^cY
!v705!S
!v706!p
!v708!62
!v709!article
!v880!S0034-89102004000300019
!v882!^v38^n3
